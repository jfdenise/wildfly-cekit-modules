<!-- TODO Rename this module to something matching your chosen subsystem name -->
<module xmlns="urn:jboss:module:1.9" name="org.wildfly.cloud.bootable.runtime">
  <properties>
    <property name="jboss.api" value="private"/>
  </properties>

  <resources>
    <artifact name="${org.wildfly.cloud:wildfly-bootable-cloud-configurator}"/>
  </resources>
  <dependencies>
    <module name="org.wildfly.bootable-jar"/>
    <module name="java.xml"/>
  </dependencies>
  <provides>
    <service name="org.wildfly.core.jar.runtime.BootableServerConfigurator">
      <with-class name="org.wildfly.cloud.bootable.runtime.CloudConfigurator"/>
    </service>
  </provides>
</module>